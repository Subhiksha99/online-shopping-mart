<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <display-name>Online Shopping Mart</display-name>
   <welcome-file-list>  
   <welcome-file>View/jsp/design.jsp</welcome-file>     
  </welcome-file-list>
    <servlet>
        <servlet-name>computers</servlet-name>
        <servlet-class>computers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>view</servlet-name>
        <servlet-class>view</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signin</servlet-name>
        <servlet-class>signin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searching</servlet-name>
        <servlet-class>searching</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registerComplaint</servlet-name>
        <servlet-class>registerComplaint</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>computers</servlet-name>
        <url-pattern>/computers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>view</servlet-name>
        <url-pattern>/view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signin</servlet-name>
        <url-pattern>/signin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searching</servlet-name>
        <url-pattern>/searching</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registerComplaint</servlet-name>
        <url-pattern>/registerComplaint</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
